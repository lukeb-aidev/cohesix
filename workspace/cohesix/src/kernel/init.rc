# Cohesix kernel boot script
# Mount the boot volume and wire BusyBox applets

mount /srv/boot /boot
bind -a /boot/bin /bin
bind -a /boot/usr/bin /usr/bin

applets='echo ls cp mv cat grep head tail printf test mkdir rmdir vi mount sh ash'
for(app in $applets){
    bind -c /bin/busybox /bin/$app
}

echo Cohesix init.rc executed
