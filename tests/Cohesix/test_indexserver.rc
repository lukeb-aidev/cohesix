#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_indexserver.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/indexserver
if(! test -x $bin){
    echo 'indexserver missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'indexserver ok'
}else{
    echo 'indexserver invocation failed' >[1=2]
    exit 'FAIL'
}
