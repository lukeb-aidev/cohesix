#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_exportfs.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/exportfs
if(! test -x $bin){
    echo 'exportfs missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'exportfs ok'
}else{
    echo 'exportfs invocation failed' >[1=2]
    exit 'FAIL'
}
