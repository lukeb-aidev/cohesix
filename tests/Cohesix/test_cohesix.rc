#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohesix.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohesix
if(! test -x $bin){
    echo 'cohesix missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohesix ok'
}else{
    echo 'cohesix invocation failed' >[1=2]
    exit 'FAIL'
}
