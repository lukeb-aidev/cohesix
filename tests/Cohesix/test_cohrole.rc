#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohrole.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohrole
if(! test -x $bin){
    echo 'cohrole missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohrole ok'
}else{
    echo 'cohrole invocation failed' >[1=2]
    exit 'FAIL'
}
