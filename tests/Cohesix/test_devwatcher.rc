#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_devwatcher.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/devwatcher
if(! test -x $bin){
    echo 'devwatcher missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'devwatcher ok'
}else{
    echo 'devwatcher invocation failed' >[1=2]
    exit 'FAIL'
}
