#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohbuild.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohbuild
if(! test -x $bin){
    echo 'cohbuild missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohbuild ok'
}else{
    echo 'cohbuild invocation failed' >[1=2]
    exit 'FAIL'
}
