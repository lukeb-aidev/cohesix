#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohup.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohup
if(! test -x $bin){
    echo 'cohup missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohup ok'
}else{
    echo 'cohup invocation failed' >[1=2]
    exit 'FAIL'
}
