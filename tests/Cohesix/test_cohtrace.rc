#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohtrace.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohtrace
if(! test -x $bin){
    echo 'cohtrace missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohtrace ok'
}else{
    echo 'cohtrace invocation failed' >[1=2]
    exit 'FAIL'
}
