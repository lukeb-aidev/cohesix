#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohfuzz.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohfuzz
if(! test -x $bin){
    echo 'cohfuzz missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohfuzz ok'
}else{
    echo 'cohfuzz invocation failed' >[1=2]
    exit 'FAIL'
}
