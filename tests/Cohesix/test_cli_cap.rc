#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cli_cap.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cli_cap
if(! test -x $bin){
    echo 'cli_cap missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cli_cap ok'
}else{
    echo 'cli_cap invocation failed' >[1=2]
    exit 'FAIL'
}
