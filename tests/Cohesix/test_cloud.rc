#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cloud.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cloud
if(! test -x $bin){
    echo 'cloud missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cloud ok'
}else{
    echo 'cloud invocation failed' >[1=2]
    exit 'FAIL'
}
