#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_scenario_compiler.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/scenario_compiler
if(! test -x $bin){
    echo 'scenario_compiler missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'scenario_compiler ok'
}else{
    echo 'scenario_compiler invocation failed' >[1=2]
    exit 'FAIL'
}
