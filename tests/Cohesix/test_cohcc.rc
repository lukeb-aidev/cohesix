#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohcc.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohcc
if(! test -x $bin){
    echo 'cohcc missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohcc ok'
}else{
    echo 'cohcc invocation failed' >[1=2]
    exit 'FAIL'
}
