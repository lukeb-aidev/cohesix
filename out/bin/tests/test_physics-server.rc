#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_physics-server.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/physics-server
if(! test -x $bin){
    echo 'physics-server missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'physics-server ok'
}else{
    echo 'physics-server invocation failed' >[1=2]
    exit 'FAIL'
}
