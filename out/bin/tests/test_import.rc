#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_import.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/import
if(! test -x $bin){
    echo 'import missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'import ok'
}else{
    echo 'import invocation failed' >[1=2]
    exit 'FAIL'
}
