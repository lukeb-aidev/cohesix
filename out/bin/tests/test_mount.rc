#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_mount.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/mount
if(! test -x $bin){
    echo 'mount missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'mount ok'
}else{
    echo 'mount invocation failed' >[1=2]
    exit 'FAIL'
}
