#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohrun_cli.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohrun_cli
if(! test -x $bin){
    echo 'cohrun_cli missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohrun_cli ok'
}else{
    echo 'cohrun_cli invocation failed' >[1=2]
    exit 'FAIL'
}
