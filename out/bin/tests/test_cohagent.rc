#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohagent.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohagent
if(! test -x $bin){
    echo 'cohagent missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohagent ok'
}else{
    echo 'cohagent invocation failed' >[1=2]
    exit 'FAIL'
}
