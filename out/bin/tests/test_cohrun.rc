#!/bin/rc
# CLASSIFICATION: COMMUNITY
# Filename: test_cohrun.rc v0.1
# Author: Lukas Bower
# Date Modified: 2027-12-07

bin=/bin/cohrun
if(! test -x $bin){
    echo 'cohrun missing or not executable' >[1=2]
    exit 'FAIL'
}
$bin -h >/dev/null >[2]/dev/null
if(~ $#status 0){
    echo 'cohrun ok'
}else{
    echo 'cohrun invocation failed' >[1=2]
    exit 'FAIL'
}
