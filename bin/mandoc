#!/bin/sh
# CLASSIFICATION: COMMUNITY
# Filename: mandoc v0.2
# Author: Lukas Bower
# Date Modified: 2025-06-22
ARCH=$(uname -m)
DIR="$(dirname "$0")/../prebuilt/mandoc"
BIN="$DIR/mandoc.$ARCH"
if [ ! -x "$BIN" ]; then
    echo "mandoc binary missing for $ARCH. Run scripts/build_mandoc.sh" >&2
    exit 1
fi
exec "$BIN" "$@"
