# Map the root filesystem to /
bind / /

# Add Cohesix binaries into the search path
bind /usr/coh/bin /bin

# Serve namespace entries from /srv with cleanup (-c)
srv -c /srv

# Use the served namespace at /srv
mount /srv /srv
mount /srv/telemetry /telemetry
mount /srv/shell_out /shell_out
mount /home/cohesix /home/cohesix
mount /srv/cuda /srv/cuda
