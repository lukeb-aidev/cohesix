# Example init configuration demonstrating valid keys
# init_mode controls the startup safety level ("safe" or "full")
# network toggles the network stack ("on" or "off")
# start_services lists additional services to spawn

init_mode = safe
network = off
start_services = ["shell", "monitor"]
