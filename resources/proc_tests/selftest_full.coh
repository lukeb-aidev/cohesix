# Author: Lukas Bower
# Purpose: Full in-session selftest script for cohsh.
ping
EXPECT OK
cat /proc/boot
EXPECT OK
ls /proc
EXPECT OK
echo test-ack-1 > /log/queen.log
EXPECT OK
echo test-ack-2 > /log/queen.log
EXPECT OK
cat /log/queen.log
EXPECT OK
tail /log/queen.log
EXPECT OK
WAIT 200
spawn heartbeat ticks=2 ttl_s=30
EXPECT OK
tail /shard/13/worker/worker-1/telemetry
EXPECT OK
WAIT 200
kill worker-1
EXPECT OK
quit
